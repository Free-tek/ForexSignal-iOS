# platform :ios, '9.0'

target 'InstantProfits' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for InstantProfits
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'lottie-ios'
  pod 'Firebase/Messaging'

end

